package codeptit;

import java.util.Scanner;


public class CodePtit {
    public static Scanner ip = new Scanner(System.in);
    
    public static void main(String[] args) {
      int t, Height, S, N;
      t = ip.nextInt();
      while(t-->0) {
          N = ip.nextInt();
          Height = ip.nextInt();    
          for(double i = 1d; i < N; ++i) {
              System.out.printf("%.6f ", Math.sqrt(i/N)*Height);
          }
          System.out.println("");
      }
    }
}
